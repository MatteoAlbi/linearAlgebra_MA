{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "${workspaceFolder}/src",
            "/usr/include/c++/13"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/gcc-13",
            "intelliSenseMode": "linux-gcc-x64",
            "includePath": [
                "${myIncludePath}",
                "/usr/include",
                "/usr/include/**",
                "build/GCC 13.1.0 x86_64-linux-gnu/Debug/_deps/googletest-src/googletest/include",
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/include/**"
            ],
            "cStandard": "gnu11",
            "cppStandard": "gnu++20",
            "browse": {
                "path": [
                    "${myIncludePath}",
                    "/usr/include",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}